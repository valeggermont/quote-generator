{
  "name": "quote-generator",
  "version": "1.0.0",
  "description": "Qualifio's quote generator",
  "main": "build/index.js",
  "scripts": {
    "build:server": "tsc",
    "build:app": "webpack --config webpack.config.ts",
    "build": "npm run build:server && npm run build:app",
    "start": "node build/index.js",
    "start:dev": "concurrently \"nodemon src/index.ts\" \"webpack --config webpack.config.ts --watch\"",
    "lint": "eslint src/** --max-warnings=0",
    "lint:fix": "eslint src/** --fix",
    "test": "jest --coverage"
  },
  "author": "Valentin Eggermont",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.21.3",
    "react-router-dom": "^6.21.3",
    "recoil": "^0.7.7",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.2",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2"
  }
}
